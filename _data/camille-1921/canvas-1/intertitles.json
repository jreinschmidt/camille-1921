      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Spring. Away from the old life, Armand's tender care proved that love is the greatest doctor.",
        "format": "text/plain"
      },
      "target": {
        "source": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "1298.28,1309.07"
