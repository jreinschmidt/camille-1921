      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Autumn. The Hazard d'Or, the smartest gaming place in Paris.",
        "format": "text/plain"
      },
      "target": {
        "source": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas",
