      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Summer. Secrets--schemes--sacrifices for future happiness.",
        "format": "text/plain"
      },
      "target": {
        "source": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas",
        "selector": {
