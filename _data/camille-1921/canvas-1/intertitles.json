      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Over Marguerite fell the shadow of a vague presentiment. She was Manon and Armand was--",
        "format": "text/plain"
      },
      "target": {
        "source": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas",
