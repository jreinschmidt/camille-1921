      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "But his boyhood friend, Gaston Rieux, had grown blas√©. ...Rex Cherryman",
        "format": "text/plain"
      },
      "target": {
        "source": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
