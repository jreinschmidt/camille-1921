    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas/intertitles-annotation-5.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "The Lady with the Camellias. She was a useless ornament--a plaything--a bird of passage--a momentary aurora. Marguerite Gautier, Nazimova",
        "format": "text/plain"
      },
      "target": {
        "source": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "161.24,173.0"
