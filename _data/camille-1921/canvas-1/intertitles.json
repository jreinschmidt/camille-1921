    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas/intertitles-annotation-6.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Prudence was a sponger on her younger and more successful sisters. ...Zeffie Tilbury",
        "format": "text/plain"
      },
      "target": {
        "source": "https://jreinschmidt.github.io/camille-1921/camille-1921/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "306.26,314.25"
